set tabsize 4
set tabstospaces
set smooth
#set autoindent
set morespace
set cutfromcursor

bind ^Space mark main
#bind ^C copytext main
#bind ^V uncut main

bind ^F whereis main
bind ^H replace main

bind ^G gotoline main

bind ^Z undo main
bind ^Y redo main

# Fix for pasting data from Alacritty
bind ^J enter main

